{
  "name": "cookievoting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predev": "bash scripts/sync-agent-rules.sh --auto",
    "build": "tsc --noEmit && vite build",
    "prebuild": "bash scripts/sync-agent-rules.sh --auto",
    "lint": "eslint .",
    "format": "prettier --write .",
    "preview": "vite preview",
    "test": "jest --config config/jest.config.cjs",
    "test:integration": "jest --config config/jest.config.cjs --testPathPattern=tests/integration",
    "test:detection": "jest --config config/jest.config.cjs --testPathPattern=cookie-detection",
    "test:detection:accuracy": "jest --config config/jest.config.cjs --testPathPattern=cookie-detection-accuracy",
    "test:e2e": "playwright test --config config/playwright.config.ts",
    "test:all": "npm run test:integration && npm run test:e2e",
    "test:e2e:ui": "playwright test --config config/playwright.config.ts --ui",
    "test-storybook": "vitest run --project=storybook",
    "test:coverage": "vitest run --coverage",
    "playwright:install": "playwright install",
    "firebase:setup": "node scripts/setup-firebase.js",
    "firebase:login": "firebase login",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules,storage",
    "firebase:deploy:firestore": "firebase deploy --only firestore:rules",
    "firebase:deploy:storage": "firebase deploy --only storage",
    "firebase:deploy:functions": "npm run build --prefix functions && firebase deploy --only functions",
    "firebase:deploy:hosting": "firebase deploy --only hosting",
    "firebase:deploy:all": "firebase deploy",
    "deploy": "npm run build && firebase deploy --only hosting",
    "emulators:start": "node scripts/start-emulators.js --only auth,firestore,storage,functions",
    "emulators:start:seed": "node scripts/start-emulators.js --only auth,firestore,storage,functions --import=./emulator-data",
    "emulators:start:fresh": "node scripts/start-emulators.js --only auth,firestore,storage,functions --export-on-exit=emulator-data",
    "emulators:seed": "node scripts/seed-emulator-data.js",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:export:manual": "node scripts/export-emulator-data.js",
    "emulators:import:manual": "node scripts/import-emulator-data.js",
    "emulators:clear": "node scripts/clear-emulator-data.js",
    "emulators:fetch-prod": "node scripts/fetch-prod-data.js",
    "emulators:make-admin": "node scripts/make-admin.js",
    "emulators:refresh-from-prod": "npm run emulators:fetch-prod && firebase emulators:exec --export-on-exit=emulator-data \"node scripts/import-emulator-data.js\"",
    "emulators:exec": "firebase emulators:exec --only auth,firestore,storage,functions",
    "test:integration:emulator": "FIREBASE_EMULATOR=true npm run test:integration",
    "detect:all-images": "node scripts/detect-all-images.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "verify": "node scripts/verify.js",
    "sync-agent-rules": "bash scripts/sync-agent-rules.sh",
    "sync-agent-rules:check": "bash scripts/sync-agent-rules.sh --check"
  },
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^2.2.3",
    "@tensorflow/tfjs": "^4.22.0",
    "@types/react-datepicker": "^6.2.0",
    "@types/uuid": "^10.0.0",
    "firebase": "^12.7.0",
    "react": "^19.2.0",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.1",
    "react-zoom-pan-pinch": "^3.7.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@google/generative-ai": "^0.24.1",
    "@playwright/test": "^1.48.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-onboarding": "^10.1.10",
    "@storybook/addon-vitest": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.1.10",
    "firebase-admin": "^13.0.1",
    "firebase-tools": "^15.1.0",
    "globals": "^16.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.57.0",
    "prettier": "^3.7.4",
    "storybook": "^10.1.10",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
